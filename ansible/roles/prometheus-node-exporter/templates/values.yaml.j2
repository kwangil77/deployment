namespaceOverride: "{{ release_namespace }}"
image:
  repository: quay.io/prometheus/node-exporter
prometheus:
  monitor:
    enabled: true
    relabelings:
    - action: replace
      regex: (.*)
      replacement: $1
      sourceLabels:
      - __meta_kubernetes_pod_node_name
      targetLabel: kubernetes_node
tolerations:
- operator: Exists